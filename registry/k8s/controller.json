{
  "kind":"ReplicationController",
  "apiVersion":"v1",
  "metadata":{
    "name":"registry",
    "labels":{
      "name":"registry"
    }
  },
  "spec":{
    "replicas":1,
    "selector":{
      "name":"registry"
    },
    "template":{
      "metadata":{
        "labels":{
          "name":"registry"
        }
      },
      "spec":{
        "containers":[
          {
            "image":"kope/registry:latest",
            "name":"registry",
            "ports":[
              {
                "name":"registry",
                "containerPort":5000,
                "protocol":"TCP"
              }
            ]
          }
        ]
      }
    }
  }
}