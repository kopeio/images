{
  "kind":"ReplicationController",
  "apiVersion":"v1",
  "metadata":{
    "name":"memcached",
    "labels":{
      "name":"memcached"
    }
  },
  "spec":{
    "replicas":1,
    "selector":{
      "name":"memcached"
    },
    "template":{
      "metadata":{
        "labels":{
          "name":"memcached"
        }
      },
      "spec":{
        "containers":[
          {
            "image":"gcr.io/kubernetes-e2e-justinsb/kope-memcached:latest",
            "name":"memcached",
            "ports":[
              {
                "name":"memcached",
                "containerPort":11211,
                "protocol":"TCP"
              }
            ]
          }
        ]
      }
    }
  }
}