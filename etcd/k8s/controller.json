{
  "kind":"ReplicationController",
  "apiVersion":"v1",
  "metadata":{
    "name":"etcd",
    "labels":{
      "name":"etcd"
    }
  },
  "spec":{
    "replicas":1,
    "selector":{
      "name":"etcd"
    },
    "template":{
      "metadata":{
        "labels":{
          "name":"etcd"
        }
      },
      "spec":{
        "containers":[
          {
            "image":"kope/etcd:latest",
            "name":"etcd",
            "ports":[
              {
                "name":"etcd",
                "containerPort":4001,
                "protocol":"TCP"
              }
            ]
          }
        ]
      }
    }
  }
}